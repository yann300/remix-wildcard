{
  "name": "remix-wildcard",
  "version": "0.0.0",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist",
    "start:dev": "nodemon --config nodemon.json",
    "test": "nodemon --config nodemon.test.json",
    "start:prod": "NODE_ENV=production pm2 start --name remix npm -- run start",
    "stop:prod": "pm2 stop remix",
    "delete:prod": "pm2 delete remix",
    "restart:prod": "npx pm2 stop remix; npx pm2 delete remix; npm run build && npm run start:prod",
    "update": "git fetch origin master && git checkout origin/master"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "@types/express-http-proxy": "^1.6.0",
    "@types/vhost": "^3.0.1",
    "command-line-args": "^5.1.1",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-http-proxy": "^1.6.0",
    "helmet": "^3.22.0",
    "http-proxy-middleware": "^2.0.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "vhost": "^3.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.5.10",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^9.0.0",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "nodemon": "^2.0.3",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.10.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
